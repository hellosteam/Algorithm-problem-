#include <iostream>
#include <cstring>
#include <algorithm>
using namespace std;
 int n,m;
const int N=100010,M=1000010;
int ne[N];
char S[M],P[N];
int main()
{   cin>>n>>P+1>>m>>S+1;
    for(int i=2,j=0;i<=n;i++)
    {
        while(j&&P[i]!=P[j+1]) j=ne[j];
        if(P[i]==P[j+1]) j++;
        ne[i]=j;
    }
    for(int i=1,j=0;i<=m;i++)
    {
        while(j&&S[i]!=P[j+1]) j=ne[j];
        if(S[i]==P[j+1]) j++;
        if(j==n)
        {
            printf("%d ",i-n);
            j=ne[j];
        }
    }
    return 0;
}
